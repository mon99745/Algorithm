import java.util.Arrays;
class Solution {
    public int solution(int[] arr) {
        int answer = 0;
        Arrays.parallelSort(arr);

        int tmp = arr[0];       
        for ( int i=0; i<arr.length; i++ ) {
            tmp = lcm(tmp, arr[i]);
        }
        answer = tmp;
        System.out.println(answer);

        return answer;
    }

    static int lcm( int a, int b ) {
        return a*b / gcd(a, b);
    }

    static int gcd( int a, int b ) {

        while (b!=0) {
            int c = a%b;
            a=b;
            b=c;
        }

        return a;
    }

}
